{
  "env": {
    "browser": true,
    "es6": true,
    "react-native/react-native": true
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react",
    "plugin:prettier/recommended",
    "eslint-config-prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react",
    "prettier",
    "eslint-plugin-react",
    "eslint-plugin-prettier"
  ]
}
